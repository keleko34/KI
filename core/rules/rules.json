//(.*?) = wildcard word
//(.*) = wildcard code
//(\d*) = wildcard integer
//(\w*) = wildcard string

{
  'keywords':['function','var','global','if','else if','else'],
  'math':{
    '+':'function(a,b){return (a+b);};',
    '-':'function(a,b){return (a-b);};',
    '*':'function(a,b){return (a*b);};',
    '/':'function(a,b){return (a/b);};',
    '%':'function(a,b){return (a%b);};'
  },
  'comporators':{
    '===':'function(a,b){return (a === b);};',
    '!==':'function(a,b){return (a !== b);};',
    '>':'function(a,b){return (a > b);};',
    '<':'function(a,b){return (a < b);};',
    '>=':'function(a,b){return (a >= b);};',
    '<=':'function(a,b){return (a <= b);};'
  },
  types:['string','number','function','object','undefined'],
  'relations':{

  },
  'snippets':{
    'global':'global.(.*?) = (.*);',
    'function':'function((.*?)){(.*)};',
    'var':'var (.*?) = (.*);'
  }
}
